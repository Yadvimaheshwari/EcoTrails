{
  "expo": {
    "name": "EcoAtlas",
    "slug": "ecoatlas-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "54.0.0",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2D4739"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "ecoatlas-mobile"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ecoatlas.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2D4739"
      },
      "package": "com.ecoatlas.mobile"
    },
    "plugins": [
      "expo-location",
      "expo-camera",
      "expo-asset",
      [
        "react-native-maps",
        {
          "googleMapsApiKey": ""
        }
      ]
    ]
  }
}
